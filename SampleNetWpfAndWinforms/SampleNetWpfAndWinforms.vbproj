  <ItemGroup>
    <ProjectReference Include="..\Nukepayload2.SourceGenerators.MyWpfExtension\Nukepayload2.SourceGenerators.MyWpfExtension.vbproj" OutputItemType="Analyzer" ReferenceOutputAssembly="False" />
  </ItemGroup>
  
</Project>
